<div class="container-fluid">
  <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
    <!-- contents -->
    <div class="panel-group" id="accordion">
    <% data.forEach(function(data) { %>
      <div class="panel panel-default">
        <div class="panel-heading">
          <h4 class="panel-title">
            <a data-toggle="collapse" href="#collapse<%= data.id %>">
            <%= data.title %></a>
          </h4>
        </div>
        <div id="collapse<%= data.id%>" class="panel-collapse collapse">
          <div class="panel-body">
          <!-- subtopics start -->

            <% data.subtopics.forEach(function(subtopic) { %>
            <div class="panel panel-default">
              <div class="panel-heading">
                <h4 class="panel-title">
                  <a data-toggle="collapse" href="#collapse<%= subtopic.id %>-subtopic">
                  <%= subtopic.subtopic %></a>
                </h4>
              </div>
              <div id="collapse<%= subtopic.id%>-subtopic" class="panel-collapse collapse">
                <div class="panel-body">
                <%- subtopic.response %>

                <br>
                <a href="<%= subtopic.url %>" target="_/"><%= subtopic.source %></a>
                </div>
              </div>
            </div>
            <% }) %>

          <!-- subtopics end -->
          </div>
        </div>
      </div>
      <% }) %>
    </div>
    <!-- end contents -->
  </div>
</div>
